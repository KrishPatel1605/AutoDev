{
  "name": "autodev",
  "displayName": "AutoDev",
  "description": "Automatically detect frontend/backend and run them together",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:autodev.start"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autodev.start",
        "title": "AutoDev: Start Project"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.34.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
